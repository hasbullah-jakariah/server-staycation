<!-- DataTales Example -->
<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="pills-home-tab">
    <div class="row">
        <% for(let i = 0; i < item.imageId.length; i++){ %>
        <div class="col">
            <div class="card shadow mb-4">
                <div class="card-body">
                    <img src="https://staycation-admin.cyclic.app/<%= item.imageId[i].imageUrl %>" alt="image"
                        style="width: 100%; display: block;">
                </div>
            </div>
        </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-info shadow-lg mb-5">Back</a>
</div>